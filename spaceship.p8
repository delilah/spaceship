pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- spaceship 
-- https://ztiromoritz.github.io/pico-8-shooter/


function _init()
	t=0
	
	ship = {
		sp=1, 
		x=60, 
		y=100, 
		h=4, 
		p=0,
		t=0,
		imm=false,
		box = {x1=0, y1=0, x2=7, y2=7}
		}
	
	killed_counter = 0

	bullets = {}
	enemies = {}
	explosions = {}
	stars = {}
	
	for i=1,128 do
		add(stars, {
		x = rnd(128),
		y = rnd(128),
		s = rnd(2)+1
		})
	end
		
	start()
end

function respawn()
	local n = flr(rnd(9))+2
	for i = 1, n do
		local d = -1
		if rnd(1) < 0.5 then d=1 end
	add(enemies, {
			sp = 17,
			m_x = i*16,
			m_y = -20-i*8,
			d = d,
			x = -32,
			y = -32,
			r = 12,
			box = {x1=0, y1=0, x2=7, y2=7}
		})
	end
end

-- redefining game functions
function start()
	_update = update_game
	_draw = draw_game
end

function game_over()
	_update = update_over
	_draw = draw_over
end

function update_over()
	
end

function draw_over()
	cls()
	gameovermessage = "Œ game over! killed: "..killed_counter	
	print_centered(gameovermessage)
end

-- 64 is half of the screen width (128 / 2). to calculate the x
-- coordinate, the length of the string is multiplied by half
-- of the character width (4 / 2), then subtracted from 64.
-- similarly, the y coordinate is 60 = 64 - (8 / 2).
function print_centered(str)
  print(str, 64 - (#str * 2), 60, 4) 
end


-- collision boxes
function abs_box(s)
	local box = {}
	box.x1 = s.box.x1 + s.x
	box.y1 = s.box.y1 + s.y
	box.x2 = s.box.x2 + s.x
	box.y2 = s.box.y2 + s.y
	return box
end
	
	
-- collision function
function coll(a, b)
	--todo
	local box_a = abs_box(a)
	local box_b = abs_box(b)
	
	if box_a.x1 > box_b.x2 or
				box_a.y1 > box_b.y2 or
				box_b.x1 > box_a.x2 or
				box_b.y1 > box_a.y2 then
				return false
	end
	
	return true
end

-- explode the ships
function explode(x,y)
	add(explosions, {x=x, y=y, t=0})
end

-- fire (bullets) function
function fire()
	local b = {
		sp = 3,
		x = ship.x,
		y = ship.y,
		dx = 0,
		dy = -3,
		box = {x1=2, y1=0, x2=5, y2=4}
		}
		add(bullets, b)
end
	
function update_game()
	t = t+1
	
	-- health and immortality
	if ship.imm then
	ship.t += 1
		if ship.t > 30 then
			ship.imm = false
			ship.t = 0
		end
	end
	
	-- stars
	for st in all(stars) do
		st.y += st.s
		if st.y >= 128 then
		st.y = 0
		st.x = rnd(128)
		end
	end
	
	-- explosions
	for ex in all(explosions) do
		ex.t += 1
		if ex.t == 13 then
			del(explosions, ex)
		end
	end
	
	-- respawn
	if #enemies <= 0 then
		respawn()
	end
	
	-- move enemies
	for e in all(enemies) do
		e.m_y += 1.3
		e.x = e.r*sin(e.d*t/50) + e.m_x
		e.y = e.r*cos(t/50) + e.m_y
		-- health on collision
		if coll(ship, e) and not ship.imm then
			ship.imm = true
			ship.h -= 1
			sfx(3)
	
			if ship.h <= 0 then
				game_over()
			end
		end
				
			if e.y > 150 then
				del(enemies, e)
			end
				
	end

	
	for b in all(bullets) do
		b.x += b.dx
		b.y += b.dy
		
		if b.x < 0 or b.x > 128 or 
		b.y < 0 or b.y >128 then
		del(bullets, b)
		end
		
		for e in all(enemies) do
			if coll(b,e) then
				del(enemies,e)
				killed_counter += 1
				ship.p += 1
				explode(e.x, e.y)
			end
		end
	end 
	
		
	if(t%6<3) then
		ship.sp=1
	else
		ship.sp=2	
	end
	
	-- basic ship movement
		if btn(0) and ship.x > 1 then ship.x -= 1.3 end
		if btn(1) and ship.x < 121 then ship.x += 1.3 end
		if btn(2) and ship.y > 0 then ship.y -= 1.3 end
		if btn(3) and ship.y < 120 then ship.y += 1.3 end
		if btnp(4) then 
			sfx(2)
			fire() 
		end
	
end


function draw_game()
	cls()
	-- stars
	for st in all(stars) do
		pset(st.x, st.y, 6)
	end
	
	print(ship.p,9)
	
	if not ship.imm or t%8 < 4 then
		spr(ship.sp, ship.x, ship.y)
	end
	
	
	for ex in all(explosions) do
		sfx(1)
		circ(ex.x, ex.y, ex.t/2, 8 + ex.t%3)
	end
	
	-- fire laser bullets
	for b in all(bullets) do
		spr(b.sp, b.x, b.y)
	end
	
	-- draw enemies
	for e in all(enemies) do
		spr(e.sp, e.x, e.y)
	end
	
	-- draw hearts
	for i=1,4 do
		if i <= ship.h then
			spr(33, 80+6*i, 3)
		else
			spr(34, 80+6*i, 3)
		end
	end
	
end
